{
  "name": "hue-explorer",
  "version": "0.2.0",
  "private": true,
  "homepage": "http://hue-explorer.herokuapp.com/",
  "proxy": {
    "/(oauth2|bridge)": {
      "target": "https://api.meethue.com",
      "changeOrigin": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "flow": "flow",
    "precommit": "pretty-quick --staged",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "flow-bin": "^0.74.0",
    "husky": "^0.14.3",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1"
  }
}
